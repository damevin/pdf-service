{
  "name": "@keybas-engineering/pdf-service",
  "version": "1.0.0",
  "description": "HTML to PDF conversion server.",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc && tsc-alias -f -p tsconfig.json",
    "format": "npm run lint:format -- --write",
    "lint:ci": "cross-env ESLINT_CODE_QUALITY_REPORT=gl-codequality.json eslint --format gitlab --ext .ts,.json .",
    "lint:eslint": "eslint --ext .ts,.json .",
    "lint:format": "prettier --check --ignore-path .gitignore \"**/*.+(ts|json)\"",
    "start": "node build/src/server.js",
    "test": "c8 mocha src/routes/**/*.spec.ts test/unit/**/*.spec.ts",
    "watch": "ts-node-dev --respawn --rs --tree-kill --debounce 1000 -r tsconfig-paths/register --ignore-watch node_modules --transpile-only src/server.ts"
  },
  "author": "Keybas",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/keybas-engineering/pdf-service.git"
  },
  "bugs": {
    "url": "https://github.com/keybas-engineering/pdf-service/issues"
  },
  "homepage": "https://github.com/keybas-engineering/pdf-service#readme",
  "devDependencies": {
    "@france-atelier/eslint-config-backend": "^1.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.26",
    "@types/symlink-or-copy": "^1.2.0",
    "@typescript-eslint/parser": "^5.13.0",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-formatter-gitlab": "^3.0.0",
    "fishery": "^2.2.2",
    "json-schema-to-ts": "^1.6.5",
    "mocha": "^9.2.1",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "pino-pretty": "^7.5.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.3.10",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "envalid": "^7.3.0",
    "fastify": "^3.27.2",
    "fastify-cors": "^6.0.3",
    "fastify-error": "^1.0.0",
    "fastify-formbody": "^5.1.0",
    "fastify-helmet": "^7.0.1",
    "fastify-metrics": "^8.0.0",
    "fastify-multer": "^2.0.2",
    "fastify-now": "^2.5.2",
    "fastify-rate-limit": "^5.7.2",
    "fastify-sensible": "^3.1.1",
    "fastify-swagger": "^4.17.1",
    "get-stream": "^6.0.1",
    "http-graceful-shutdown": "^3.1.6",
    "is-docker": "^2.2.1",
    "param-case": "^3.0.4",
    "symlink-or-copy": "^1.3.1",
    "under-pressure": "^5.8.0"
  }
}
